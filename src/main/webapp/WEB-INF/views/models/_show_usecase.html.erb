
<table class="table table-striped table-bordered">
    <tr>
        <th>ID </th>
        <th>Name</th>
        <th>Author</th>
        <th>Update time</th>
        <th>Action</th>
    </tr>
    <% @process_informations.each do |process_information| %>
        <% if process_information.is_shared == true %>
        <tr>
            <td>
                <%= process_information.id %>
            </td>
            <td>
                    <%= process_information.name%>
            </td>
            <td>
                <p>Admin</p>
            </td>
            <td>
                <%= process_information.updated_at.strftime("%Y-%m-%d %H:%M:%S")  %>
            </td>
            <td>
                <%= link_to process_copy_path(:process_id => process_information.id), method: :post, class: "btn btn-info btn-sm", role:"button" do %>
                    <span class="glyphicon glyphicon-play" aria-hidden="true"></span> Execute
                <% end %>
            </td>
        </tr>
    <% end %>
<% end %>
</table>
